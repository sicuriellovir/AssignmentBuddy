<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.softwareengrproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PrivacyMessages"></activity>
        <activity android:name=".PrivacyNotif" />
        <activity android:name=".NotificationsScreen" />
        <activity
            android:name=".SettingsActivityPage"
            android:label="@string/title_settings_activity" />

        <provider
            android:name=".AcctDB"
            android:authorities="com.example.softwareengrproject.provider"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".LoginScreen" />
        <activity android:name=".HomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>